    {
      "name": "Zen MCP Server",
      "description": "Supercharge Claude Code/GeminiCLI/CodexCLI with multiple AI models (Gemini/OpenAI/OpenRouter/Azure/Grok/Ollama) working as one",
      "official_package": "zen-mcp-server",
      "github_url": "https://github.com/BeehiveInnovations/zen-mcp-server",
      "configuration": {
        "command": "bash",
        "args": [
          "-c",
          "for p in $(which uvx 2>/dev/null) $HOME/.local/bin/uvx /opt/homebrew/bin/uvx /usr/local/bin/uvx uvx; do [ -x \"$p\" ] && exec \"$p\" --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git zen-mcp-server; done; echo 'uvx not found' >&2; exit 1"
        ],
        "env": {
          "PATH": "/usr/local/bin:/usr/bin:/bin:/opt/homebrew/bin:~/.local/bin",
          "GEMINI_API_KEY": "your-key-here",
          "OPENAI_API_KEY": "optional",
          "OPENROUTER_API_KEY": "optional",
          "AZURE_OPENAI_API_KEY": "optional",
          "DISABLED_TOOLS": "analyze,refactor,testgen,secaudit,docgen,tracer",
          "DEFAULT_MODEL": "auto"
        }
      },
      "installation": {
        "method": "git + uvx",
        "quick_setup": "./run-server.sh",
        "one_line": "exec $(which uvx || echo uvx) --from git+https://github.com/BeehiveInnovations/zen-mcp-server.git zen-mcp-server"
      },
      "requirements": {
        "api_key": true,
        "api_key_note": "At least one API key required from supported providers",
        "environment_variables": [
          "GEMINI_API_KEY",
          "OPENAI_API_KEY",
          "OPENROUTER_API_KEY",
          "AZURE_OPENAI_API_KEY"
        ],
        "python_version": "3.10+ (3.12 recommended)",
        "tools_required": ["Git", "uv"],
        "windows_note": "Requires WSL2 for Claude Code CLI"
      },
      "supported_providers": [
        "OpenRouter (multiple models)",
        "Gemini (Google AI Studio)",
        "OpenAI (O3, GPT-5)",
        "X.AI (Grok models)",
        "DIAL Platform"
      ],
      "configuration_options": {
        "DISABLED_TOOLS": "Comma-separated list of tools to disable",
        "DEFAULT_MODEL": "Options: 'auto', 'pro', or specific model names",
        "LOG_LEVEL": "Logging verbosity",
        "CONVERSATION_TIMEOUT_HOURS": "Session timeout",
        "MAX_CONVERSATION_TURNS": "Maximum turns per conversation"
      }
    },
