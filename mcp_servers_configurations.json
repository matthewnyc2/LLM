      "name": "Context7 MCP Server",
      "description": "Up-to-date code documentation for LLMs and AI code editors by Upstash",
      "official_package": "@upstash/context7-mcp",
      "github_url": "https://github.com/upstash/context7",
      "configuration_local": {
        "command": "npx",
        "args": ["-y", "@upstash/context7-mcp", "--api-key", "YOUR_API_KEY"],
        "env": {}
      },
      "configuration_remote": {
