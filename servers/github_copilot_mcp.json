{
  "windows": {
    "servers": {
      "supabase": {
        "type": "http",
        "url": "https://server.smithery.ai/supabase/mcp",
        "headers": {
          "SMITHERY_PROFILE": "${input:smithery_profile}",
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "ref": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "ref-tools-mcp"
        ]
      },
      "exa": {
        "type": "http",
        "url": "https://server.smithery.ai/exa/mcp",
        "headers": {
          "SMITHERY_PROFILE": "${input:smithery_profile}",
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "sequential-thinking": {
        "type": "http",
        "url": "https://server.smithery.ai/@smithery-ai/server-sequential-thinking/mcp"
      },
      "browserbase": {
        "type": "http",
        "url": "https://server.smithery.ai/@browserbasehq/mcp-browserbase/mcp",
        "headers": {
          "SMITHERY_PROFILE": "${input:smithery_profile}",
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "notion": {
        "type": "http",
        "url": "https://server.smithery.ai/notion/mcp",
        "headers": {
          "SMITHERY_PROFILE": "${input:smithery_profile}",
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "clear-thought-mcp": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@ThinkFar/clear-thought-mcp"
        ],
        "env": {
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "gmail-mcp": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@shinzo-labs/gmail-mcp"
        ],
        "env": {
          "SMITHERY_API_KEY": "${input:smithery_api_key}",
          "SMITHERY_PROFILE": "${input:smithery_profile}"
        }
      },
      "playwright-mcp": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "@smithery/cli@latest",
          "run",
          "@playwright/mcp"
        ],
        "env": {
          "SMITHERY_API_KEY": "${input:smithery_api_key}"
        }
      },
      "context7": {
        "type": "http",
        "url": "https://mcp.context7.com/mcp"
      },
      "serena": {
        "type": "http",
        "url": "http://localhost:8765/sse",
        "timeout": 10000
      },
      "zen": {
        "command": "uvx",
        "args": [
          "--from",
          "git+https://github.com/BeehiveInnovations/zen-mcp-server.git",
          "zen-mcp-server"
        ]
      },
      "playwright": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "@playwright/mcp"
        ]
      },
      "desktop-commander": {
        "command": "npx.cmd",
        "args": [
          "-y",
          "@wonderwhy-er/desktop-commander"
        ]
      },
      "gemini-cli-server": {
        "command": "gemini.cmd",
        "args": [
          "mcp",
          "server"
        ]
      }
    },
    "inputs": [
      {
        "type": "promptString",
        "id": "smithery_profile",
        "description": "Smithery Profile",
        "password": false
      },
      {
        "type": "promptString",
        "id": "smithery_api_key",
        "description": "Smithery API Key",
        "password": true
      }
    ]
  },
